{
  "redirects": [
    {
      "source": "/:path((?!www\\.).*)",
      "has": [
        {
          "type": "host",
          "value": "frontio.com.ar"
        }
      ],
      "destination": "https://www.frontio.com.ar/:path*",
      "permanent": true
    },
    {
      "source": "/:path*#:hash*",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Link",
          "value": "<https://www.frontio.com.ar/>; rel=\"canonical\""
        }
      ]
    }
  ]
}
